   "source": [
    "class Solution:\n",
    "    def isPalindrome(self, x: int) -> bool:\n",
    "        assert x >= -2**31 and x <= 2**31 - 1\n",
    "        str_x = str(x)\n",
    "        return str_x == str_x[::-1]"
   ]
  },
